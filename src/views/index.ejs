	<!-- Monaco Editor -->
	<script>var require = { paths: { 'vs': '/monaco/min/vs' } };</script>
	<script src="/monaco/min/vs/loader.js"></script>
	<script src="/monaco/min/vs/editor/editor.main.nls.js"></script>
	<script src="/monaco/min/vs/editor/editor.main.js"></script>

	<!--script src="/axios/index.js"></script-->
	<!--script src="https://unpkg.com/axios/dist/axios.min.js"></script-->

	<script>
		$(document).ready(function(){
			$(window).on('scroll', materialKit.checkScrollForTransparentNavbar);

			var editor = monaco.editor.create(document.getElementById('container'), {
				value: [
					'#include <stdio.h>',
					'',
					'int main(int argc, char* argv[])',
					'{',
					'	printf("Hello, world!\\n");',
					'	return 0;',
					'}'
				].join('\n'),
				language: 'c'
			});

			/*
			document.getElementById('student-number').addEventListener('input', function(e) {
				var id = this.value;
				var isValid = (!isNaN(id) && id.toString().length == 10);
				document.getElementById('btn-submit').disabled = isValid;
			});
			*/

			document.getElementById('btn-submit').addEventListener('click', function(e) {
				var studentNumber = document.getElementById('student-number').value;
				var code = editor.getValue();

				$.post('/submit', { studentNumber: studentNumber, code: code }, function(response) {
					console.log('response:', response);
				});
			});
		});
	</script>
